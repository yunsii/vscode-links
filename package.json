{
  "publisher": "yuns",
  "name": "links",
  "displayName": "Links",
  "version": "0.2.0",
  "private": true,
  "packageManager": "pnpm@10.11.0",
  "description": "Pursue better UX for project links",
  "author": "Yuns <yuns.xie@qq.com>",
  "license": "MIT",
  "funding": "https://opencollective.com/yunsii",
  "homepage": "https://github.com/yunsii/vscode-links#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yunsii/vscode-links"
  },
  "bugs": {
    "url": "https://github.com/yunsii/vscode-links/issues"
  },
  "sponsor": {
    "url": "https://opencollective.com/yunsii"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.92.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "title": "Links",
      "properties": {
        "links.resources": {
          "description": "Resources to open",
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "object",
            "required": [
              "url",
              "title"
            ],
            "properties": {
              "url": {
                "type": "string",
                "description": "Specific URL"
              },
              "title": {
                "type": "string",
                "description": "Title of specific URL"
              },
              "description": {
                "type": "string",
                "description": "Description of specific URL"
              }
            }
          }
        }
      }
    },
    "commands": [
      {
        "command": "links.open",
        "title": "Links Open ..."
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish",
    "prepare": "husky & nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint:fix": "eslint --fix ."
  },
  "dependencies": {
    "reactive-vscode": "^0.2.17",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@eslint-react/eslint-plugin": "^1.49.0",
    "@jannajs/lint": "^3.1.1",
    "@prettier/plugin-xml": "^3.4.1",
    "@types/node": "^22.15.18",
    "@types/vscode": "^1.92.0",
    "@vscode/vsce": "^3.4.0",
    "bumpp": "^10.1.1",
    "eslint": "9.27.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "pnpm": "^10.11.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "vscode-ext-gen": "^1.0.2"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
