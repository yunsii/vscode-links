{
  "publisher": "yuns",
  "name": "links",
  "displayName": "Links",
  "version": "0.0.9",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "description": "Pursue better UX for project links",
  "author": "Yuns <yuns.xie@qq.com>",
  "license": "MIT",
  "funding": "https://opencollective.com/yunsii",
  "homepage": "https://github.com/yunsii/vscode-links#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yunsii/vscode-links"
  },
  "bugs": {
    "url": "https://github.com/yunsii/vscode-links/issues"
  },
  "sponsor": {
    "url": "https://opencollective.com/yunsii"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "res/icon.png",
  "files": [
    "LICENSE.md",
    "dist/*",
    "res/*"
  ],
  "engines": {
    "vscode": "^1.92.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "configuration": {
      "title": "Links",
      "properties": {
        "links.resources": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "object",
            "required": [
              "url",
              "title"
            ],
            "properties": {
              "url": {
                "type": "string",
                "description": "Specific URL"
              },
              "title": {
                "type": "string",
                "description": "Title of specific URL"
              },
              "description": {
                "type": "string",
                "description": "Description of specific URL"
              }
            }
          }
        }
      }
    },
    "commands": [
      {
        "command": "links.open",
        "title": "Links Open ..."
      }
    ]
  },
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint --flag unstable_ts_config .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish",
    "prepare": "husky install & nr update",
    "update": "vscode-ext-gen --output src/generated/meta.ts",
    "lint:fix": "eslint --flag unstable_ts_config --fix ."
  },
  "dependencies": {
    "git-remote-origin-url": "^4.0.0",
    "reactive-vscode": "^0.2.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.9.1",
    "@jannajs/lint": "3.0.0-next.16",
    "@types/node": "^22.2.0",
    "@types/vscode": "^1.92.0",
    "@vscode/vsce": "^2.32.0",
    "bumpp": "^9.5.1",
    "eslint": "9.9.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "eslint-plugin-react-refresh": "^0.4.9",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.9",
    "pnpm": "^9.7.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vitest": "^2.0.5",
    "vscode-ext-gen": "^0.3.6"
  },
  "lint-staged": {
    "*": "eslint --flag unstable_ts_config --fix"
  }
}
